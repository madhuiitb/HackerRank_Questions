class Solution {
public:
    void rotate(vector<int>& nums, int k) {
        int n=nums.size();
        
         for(int i=0;i<k;i++){
             int temp = nums[n-1];
            // cout<<temp<<" ";
             for(int j=n-1;j>=0;j--){
                 if(j==0){
                     nums[j]=temp;
                     //cout<<nums[j]<<" ";
                 }
                 else
                     nums[j]=nums[j-1];
             }
         }
    }
};
